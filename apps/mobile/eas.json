{
  "cli": {
    "version": ">= 16.20.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "developmentClient": false,
      "channel": "preview",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
       "distribution": "internal"
      }
    },
    "production": {
      "developmentClient": false,
      "channel": "production",
      "autoIncrement": true,
      "android": {
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "distribution": "store"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
